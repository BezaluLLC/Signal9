{
  "AgentConfiguration": {
    "AgentFunctionsUrl": "https://localhost:7071",
    "FunctionKey": "default_local_key",
    "TenantCode": "default",
    "GroupName": "default",
    "HeartbeatInterval": 30,
    "TelemetryInterval": 60,
    "ReconnectDelay": 5,
    "MaxReconnectAttempts": 10,
    "Telemetry": {
      "EnablePerformanceMetrics": true,
      "EnableSystemHealth": true,
      "EnableEventLogs": true,
      "EnableNetworkMetrics": true,
      "EnableStorageMetrics": true,
      "PerformanceCounters": [],
      "EventLogSources": [
        "System",
        "Application"
      ],
      "CustomMetrics": {}
    },
    "Logging": {
      "LogLevel": "Information",
      "EnableConsoleLogging": true,
      "EnableFileLogging": true,
      "EnableRemoteLogging": true,
      "LogFilePath": "logs/agent.log",
      "MaxLogFileSizeMB": 10,
      "MaxLogFiles": 5
    },
    "CustomSettings": {}
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
